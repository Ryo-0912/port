<%= form_with url: login_path, method: :post do |f| %>
  <div class="field">
    <%= f.label :email %><br/>
    <%= f.text_field :email, id: 'session_email'%>
  </div>
  <div class="field">
    <%= f.label :password %><br/>
    <%= f.password_field :password, id: 'session_password' %>
  </div>

  <div class="actions">
    <%= f.submit "ログイン" %>
  </div>
<% end %>