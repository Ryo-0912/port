<%= form_with url: login_path, method: :post do |f| %>
  <div class="field">
    <p><%= f.label :email, "メールアドレス" %></p>
    <p><%= f.text_field :email, id: 'session_email', class: "session_form"%></p>
  </div>
  <div class="field">
    <p><%= f.label :password, "パスワード" %></p>
    <p><%= f.password_field :password, id: 'session_password', class: "session_form" %></p>
  </div>
  <div class="actions">
    <%= f.submit "ログイン", class: "session_form_btn" %>
  </div>
<% end %>
